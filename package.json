{
  "name": "nw-models",
  "version": "1.0.0",
  "description": "New World Models Extractor",
  "scripts": {
    "dev": "tsc -p ./tsconfig.json -w",
    "build": "tsc -p ./tsconfig.json",
    "test": "globstar -- node --loader tsx --test \"src/**/*.spec.ts\"",
    "start": "env-cmd -f .env node ./dist/index.js",
    "unpack": "yarn start unpack",
    "convert-tables": "yarn start convert-tables",
    "convert": "yarn start convert",
    "clean": "yarn start clean",
    "viewer": "http-server . -o viewer.html -c-1 --no-dotfiles"
  },
  "author": "Ginie",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
