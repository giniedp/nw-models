    "@gltf-transform/functions": "^3.5.1",
    "@types/capture-console": "^1.0.1",
    "@types/cli-progress": "^3.11.0",
    "@types/colors": "^1.2.1",
    "@types/dom-parser": "^0.1.1",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.11.12",
    "@types/npmlog": "^4.1.4",
    "@types/rimraf": "^4.0.5",
    "@types/sharp": "^0.32.0",
    "@types/shelljs": "^0.8.12",
    "@types/workerpool": "^6.4.0",
    "ansi-colors": "^4.1.3",
    "aws-sdk": "^2.1435.0",
    "babylonjs": "^6.16.0",
    "babylonjs-loaders": "^6.16.0",
    "babylonjs-serializers": "^6.16.0",
    "capture-console": "^1.0.1",
    "cli-progress": "^3.11.2",
    "colors": "^1.4.0",
    "commander": "^11.0.0",
    "dom-parser": "^0.1.6",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "fast-glob": "^3.3.1",
    "fast-xml-parser": "^4.2.7",
    "globstar": "^1.0.0",
    "http-server": "^14.1.1",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
